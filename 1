   <script type="text/javascript">
       let number = prompt
       sub = number
       let division 
       for(let i = 1; i <= number; i++) {
           sub = number 
           while (sub > 0) {
               sub -= i
               if (sub == 0)
               if (smth(i))
                division = i
           }   
       }
          console.log(division)
          function smth(i){
              let counter = 0
              let cub
              for(x = 1; x <= i; x++) {
                  sub = i
                  while(sub >0) {
                      sub -= x
                      if (sub == 0);
                           counter++;
                  }
              } 
              if (counter == 2) return true
                   if (counter > 2) return false
            }                         
